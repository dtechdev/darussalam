div.form input,
div.form textarea
{

    background:#fcfcfc;
    border: 1px solid #e3e2e2;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;



    width: 200px;
    margin-left: 0;
    margin-bottom: 3px;
    padding: 2px;
}

/*div.form select{ width:308px}*/

div.form select
{

    background:#fcfcfc;
    border: 1px solid #e3e2e2;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;


    margin-left: 0;
    margin-bottom: 3px;
    padding: 2px;
    width:208px;
}

div.form select.sideDropdown
{
    width:100px; 
}

div.form input:focus,
div.form textarea:focus,
div.form select:focus
{
    border-color: #d9d9d9 !important;
    /* -moz-box-shadow: 0 0 1px #0459b7;
     -webkit-box-shadow: 0 0 1px #0459b7;*/
    outline: none; /* Pour enlever le contour jaune lorsque l'on sï¿½lectionne un input dans Chrome */
}



