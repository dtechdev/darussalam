.btn {
    background: #07aafd;
    /* Old browsers */
    background: -moz-linear-gradient(top, #07aafd 0%, #045ab8 100%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#07aafd), color-stop(100%,#045ab8));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #07aafd 0%,#045ab8 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #07aafd 0%,#045ab8 100%); /* Opera11.10+ */
    background: -ms-linear-gradient(top, #07aafd 0%,#045ab8 100%); /* IE10+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#07aafd', endColorstr='#045ab8',GradientType=0 );
    /* IE6-9 */
    background: linear-gradient(top, #07aafd 0%,#045ab8 100%); /* W3C */


    border:1px solid #093C75;
    color: #FFFFFF;
    cursor: pointer;
    font-family: Arial,sans-serif;
    font-size: 12px;
    font-weight: bold;
    padding:2px 10px;
    text-decoration: none;
    text-shadow: 0 1px 1px #333333;
    text-transform: uppercase;
    width:auto;

}

